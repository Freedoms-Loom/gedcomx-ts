{
  "name": "gedcomx-ts",
  "version": "1.0.0",
  "description": "GEDCOM X represented in Typescript with compilation to JSON Schema. ",
  "scripts": {
    "build": "rm -rf dist/ && mkdir dist/ && ./node_modules/.bin/ts-json-schema-generator --path 'types/**/*.ts'  -o ./dist/gedcomx_schema.json --jsDoc extended --validationKeywords \"\\$id\"             "
  },
  "author": "Cornell Institute for Social and Economic Research <ciser@cornell.edu>",
  "license": "Apache License 2.0",
  "dependencies": {
    "ts-json-schema-generator": "^0.61.1"
  },
  "repository": {
    "type" : "git",
    "url" : "https://github.com/Freedoms-Loom/gedcomx-ts.git"
  },

  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  }
}
